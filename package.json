{
  "name": "api-base-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development SECURITY_ENABLED=false nest start --watch",
    "start:dev:secure": "NODE_ENV=development SECURITY_ENABLED=true nest start --watch",
    "start:debug": "NODE_ENV=development SECURITY_ENABLED=false nest start --debug --watch",
    "start:debug:secure": "NODE_ENV=development SECURITY_ENABLED=true nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "security:enable": "echo 'Security enabled for next run'",
    "security:disable": "echo 'Security disabled for next run'",
    "security:status": "echo 'Check SECURITY_ENABLED environment variable'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:html": "jest --coverage --coverageReporters=html",
    "test:cov:json": "jest --coverage --coverageReporters=json",
    "test:cov:lcov": "jest --coverage --coverageReporters=lcov",
    "test:cov:text": "jest --coverage --coverageReporters=text",
    "test:cov:all": "jest --coverage --coverageReporters=html --coverageReporters=json --coverageReporters=lcov --coverageReporters=text",
    "test:cov:open": "npm run test:cov:html && open coverage/index.html",
    "test:cov:ci": "jest --coverage --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --watchAll=false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run lint:check && npm run format:check && npm run test",
    "quality:fix": "npm run format && npm run lint && npm run test",
    "coverage:unit": "./scripts/coverage-report.sh unit",
    "coverage:e2e": "./scripts/coverage-report.sh e2e",
    "coverage:combined": "./scripts/coverage-report.sh combined",
    "coverage:html": "./scripts/coverage-report.sh html",
    "coverage:ci": "./scripts/coverage-report.sh ci",
    "coverage:clean": "./scripts/coverage-report.sh clean",
    "coverage:threshold": "./scripts/coverage-report.sh threshold",
    "coverage:badge": "./scripts/coverage-report.sh badge",
    "docker:build": "./scripts/docker-dev.sh build",
    "docker:start": "./scripts/docker-dev.sh start",
    "docker:stop": "./scripts/docker-dev.sh stop",
    "docker:restart": "./scripts/docker-dev.sh restart",
    "docker:logs": "./scripts/docker-dev.sh logs",
    "docker:shell": "./scripts/docker-dev.sh shell",
    "docker:clean": "./scripts/docker-dev.sh clean",
    "docker:status": "./scripts/docker-dev.sh status",
    "docker:db": "./scripts/docker-dev.sh db",
    "docker:test": "./scripts/docker-dev.sh test",
    "docker:lint": "./scripts/docker-dev.sh lint",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate": "npm run typeorm -- -d typeorm.config.ts migration:generate",
    "typeorm:migration:create": "npm run typeorm -- migration:create",
    "typeorm:migration:run": "npm run typeorm -- -d typeorm.config.ts migration:run",
    "typeorm:migration:revert": "npm run typeorm -- -d typeorm.config.ts migration:revert",
    "typeorm:schema:drop": "npm run typeorm -- -d typeorm.config.ts schema:drop",
    "typeorm:schema:sync": "npm run typeorm -- -d typeorm.config.ts schema:sync",
    "db:reset": "./scripts/dev-utils.sh db:reset",
    "db:seed": "./scripts/dev-utils.sh db:seed",
    "db:migrate": "./scripts/dev-utils.sh db:migrate",
    "env:setup": "./scripts/dev-utils.sh env:setup",
    "project:setup": "./scripts/dev-utils.sh project:setup",
    "project:clean": "./scripts/dev-utils.sh clean",
    "project:health": "./scripts/dev-utils.sh health",
    "migration:generate": "./scripts/dev-utils.sh generate:migration",
    "dev:reset": "npm run project:clean && npm install && npm run build",
    "dev:fresh": "npm run db:reset && npm run db:migrate && npm run db:seed",
    "ci:test": "npm run lint:check && npm run type-check && npm run test:cov:ci && npm run test:e2e:cov",
    "ci:build": "npm run build && npm run docker:build",
    "release:patch": "npm version patch && git push --tags",
    "release:minor": "npm version minor && git push --tags",
    "release:major": "npm version major && git push --tags",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@automapper/types": "^6.3.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "csrf": "^3.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "test/**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "figlet": "^1.8.2",
    "globals": "^16.3.0",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^16.1.2",
    "ora": "^8.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.38.0"
  }
}
